<odoo>
    <data>
        <record id="action_student_approved_concession_report_wizard" model="ir.actions.act_window">
            <field name="name">Students Approved Concession Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">admission.concession.report.wizard</field>
            <field name="view_id" ref="vg_colg.admission_concession_report_form"/>
            <field name="view_mode">form</field>
            <field name="context">{'default_report_type':'approved_report'}</field>
            <field name="target">new</field>
        </record>

        <menuitem id="menu_student_approved_concession_report_wizard" name="Students Approved Concession Report" action="action_student_approved_concession_report_wizard"
            parent="vg_colg.menu_parent_report_wizard" sequence="11"/>

        <record id="action_student_rejected_concession_report_wizard" model="ir.actions.act_window">
            <field name="name">Students Rejected Concession Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">admission.concession.report.wizard</field>
            <field name="view_id" ref="vg_colg.admission_concession_report_form"/>
            <field name="view_mode">form</field>
            <field name="context">{'default_report_type':'rejected_report'}</field>
            <field name="target">new</field>
        </record>

        <menuitem id="menu_student_rejected_concession_report_wizard" name="Students Rejected Concession Report" action="action_student_rejected_concession_report_wizard"
            parent="vg_colg.menu_parent_report_wizard" sequence="12"/>

    </data>
</odoo>